<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="demoJPA" transaction-type="RESOURCE_LOCAL">
		<class>co.edu.icesi.demo.modelo.Clientes</class>
		<class>co.edu.icesi.demo.modelo.Consignaciones</class>
		<class>co.edu.icesi.demo.modelo.ConsignacionesId</class>
		<class>co.edu.icesi.demo.modelo.Cuentas</class>
		<class>co.edu.icesi.demo.modelo.Retiros</class>
		<class>co.edu.icesi.demo.modelo.RetirosId</class>
		<class>co.edu.icesi.demo.modelo.TiposDocumentos</class>
		<class>co.edu.icesi.demo.modelo.TiposUsuarios</class>
		<class>co.edu.icesi.demo.modelo.Usuarios</class>
		
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/demoBanco"/>
			<property name="javax.persistence.jdbc.user" value="postgres"/>
			<property name="javax.persistence.jdbc.password" value="adminadmin"/>
			<property name="hibernate.show_sql" value="true"/>
			
		</properties>
	</persistence-unit>
</persistence>
